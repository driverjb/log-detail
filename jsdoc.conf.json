{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_|node_modules"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "destination": "docs-code",
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemName": "Log Detail",
    "footer": "<div style='text-align:center;margin-bottom:10px;'>Joel Driver | June 2020 | v3.0.0</div>",
    "copyright": "",
    "includeDate": false,
    "navType": "vertical",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": true,
    "inverseNav": false,
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "dateFormat": "YYYYMMDD - HH:mm:ss",
    "syntaxTheme": "dark",
    "sort": true,
    "search": true
  }
}
